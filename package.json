{
  "name": "pilatus_lpp_automation_test_suite",
  "version": "1.0.0",
  "description": "LPP E2E tests",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "cypress-ntlm run",
    "test:chrome": "cypress-ntlm run --browser chrome",
    "test:edge": "cypress-ntlm run --browser edge",
    "test:firefox": "cypress-ntlm run --browser firefox",
    "test:debug": "cypress-ntlm open",
    "test:load": "k6 run \"cypress/k6 performance test/K6_loadTest.js\"",
    "test:cloud": "npx cypress-ntlm run --record --key ${CYPRESS_RECORD_KEY}",
    "test:cloud:chrome": "npx cypress-ntlm run --browser chrome --record --key ${CYPRESS_RECORD_KEY}",
    "test:cloud:parallel": "npx cypress-ntlm run --record --key ${CYPRESS_RECORD_KEY} --parallel --ci-build-id date +%s",
    "test:cloud:chrome:parallel": "npx cypress-ntlm run --browser chrome --record --key ${CYPRESS_RECORD_KEY} --parallel --ci-build-id date +%s"
  },
  "author": "Nikola Nikolic",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cypress": "^13.13.0",
    "cypress-cloud": "^1.10.2",
    "cypress-delete-downloads-folder": "^0.0.5",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-ntlm-auth": "^4.1.7",
    "cypress-parallel": "^0.14.0",
    "dotenv": "^16.4.5",
    "k6": "^0.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "cypress-downloadfile": "^1.2.3"
  },
  "keywords": []
}
